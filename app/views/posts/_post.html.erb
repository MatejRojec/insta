<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= post.description %>
  </p>

  <p>
    <strong>Keywords:</strong>
    <%= post.keywords %>
  </p>

  <p>
    <strong>Images:</strong>
    <%= post.images.each do |image| %>
      <%= image_tag image, class: "img-fluid" %>
    <% end %>
  </p>

</div>
